{
  "name": "@shopify/polaris-tokens",
  "version": "5.3.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./json/*": "./dist/json/*",
    "./css/*": "./dist/css/*",
    "./scss/*": "./dist/scss/*",
    "./metadata": {
      "types": "./dist/types/metadata.d.ts",
      "import": "./dist/esm/metadata.mjs",
      "require": "./dist/cjs/metadata.js"
    }
  },
  "typesVersions": {
    "*": {
      "metadata": [
        "dist/types/metadata.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "ts-node --skip-project scripts/index.ts",
    "build:js": "rollup -c",
    "build:types": "tsc -b",
    "build:clean": "rm -rf build-internal",
    "dev": "rollup -c -w",
    "lint": "TIMING=1 eslint --cache .",
    "test": "jest",
    "clean": "rm -rf .turbo node_modules dist *.tsbuildinfo build-internal"
  },
  "devDependencies": {
    "@shopify/browserslist-config": "^3.0.0",
    "glob": "^8.0.3",
    "ts-node": "^10.7.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Shopify <dev@shopify.com>",
  "homepage": "https://polaris.shopify.com",
  "repository": "https://github.com/Shopify/polaris",
  "bugs": {
    "url": "https://github.com/Shopify/polaris/issues"
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "browserslist": [
    "extends @shopify/browserslist-config"
  ],
  "files": [
    "dist"
  ]
}
