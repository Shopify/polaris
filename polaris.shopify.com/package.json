{
  "name": "polaris.shopify.com",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "build": "yarn run gen-assets && next build",
    "postbuild": "yarn gen-sitemap",
    "dev": "node scripts/copy-icons.mjs && open http://localhost:3000 && next dev",
    "start": "next start ",
    "lint": "run-p lint:*",
    "lint:js": "TIMING=1 eslint --cache .",
    "lint:styles": "stylelint '**/*.{css,scss}'",
    "clean": "rm -rf .turbo node_modules .next *.tsbuildinfo",
    "create-component": "generact --root src/components src/components/Template/Template.tsx",
    "gen-assets": "node scripts/gen-og-images.mjs && node scripts/copy-icons.mjs",
    "gen-sitemap": "yarn next-sitemap",
    "get-props": "tsc ./scripts/get-props.ts --target esnext --module node16 --types \"react\" --outDir ./scripts/get-props && node ./scripts/get-props/scripts/get-props.js && rm -rf ./scripts/get-props"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.6.1",
    "@headlessui/react": "^1.6.5",
    "@shopify/polaris": "^9.24.0",
    "@shopify/polaris-icons": "^5.4.0",
    "@shopify/polaris-tokens": "^5.5.2",
    "codesandbox": "^2.2.3",
    "fuse.js": "^6.5.3",
    "gray-matter": "^4.0.3",
    "next": "12.1.0",
    "prismjs": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.2",
    "use-dark-mode": "^2.3.1",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^4.0.3",
    "@types/node": "17.0.21",
    "@types/prismjs": "^1.26.0",
    "@types/gtag.js": "^0.0.10",
    "@types/react": "*",
    "eslint-config-next": "12.1.0",
    "eslint": "8.10.0",
    "frontmatter": "^0.0.3",
    "generact": "^0.4.0",
    "globby": "^11.1.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.16",
    "next-sitemap": "^3.1.5",
    "puppeteer": "^16.0.0",
    "rehype-raw": "^6.1.1",
    "sass": "^1.49.9",
    "typescript": "^4.7.4"
  }
}
